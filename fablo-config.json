{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.1.0/schema.json",
  "global": {
    "fabricVersion": "2.2.4",
    "tls": true,
    "peerDevMode": false,
    "monitoring": {
      "loglevel": "info"
    },
    "tools": {
      "explorer": true
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "Inovafil",
        "domain": "inovafil.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "orderers": [
        {
          "groupName": "ordering-service",
          "prefix": "orderer",
          "type": "raft",
          "instances": 1
        }
      ],
      "ca": {
        "db": "sqlite"
      },
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "ASampaio",
        "domain": "asampaio.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "ca": {
        "db": "sqlite"
      },
      "orderers": [
        {
          "groupName": "ordering-service",
          "prefix": "orderer",
          "type": "raft",
          "instances": 1
        }
      ],
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "Tintex",
        "domain": "tintex.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "ca": {
        "db": "sqlite"
      },
      "orderers": [
        {
          "groupName": "ordering-service",
          "prefix": "orderer",
          "type": "raft",
          "instances": 1
        }
      ],
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "TMG",
        "domain": "tmg.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "orderers": [
        {
          "groupName": "ordering-service",
          "prefix": "orderer",
          "type": "raft",
          "instances": 1
        }
      ],
      "ca": {
        "db": "sqlite"
      },
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "DomingosDeSousa",
        "domain": "domingosdesousa.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "orderers": [
        {
          "groupName": "ordering-service",
          "prefix": "orderer",
          "type": "raft",
          "instances": 1
        }
      ],
      "ca": {
        "db": "sqlite"
      },
      "tools": {
        "fabloRest": true
      }
    }
  ],
  "channels": [
    {
      "name": "stvgd-channel",
      "orgs": [
        {
          "name": "Inovafil",
          "peers": ["peer0"]
        },
        {
          "name": "ASampaio",
          "peers": ["peer0"]
        },
        {
          "name": "Tintex",
          "peers": ["peer0"]
        },
        {
          "name": "TMG",
          "peers": ["peer0"]
        },
        {
          "name": "DomingosDeSousa",
          "peers": ["peer0"]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "stvgd-chaincode",
      "version": "13",
      "lang": "golang",
      "channel": "stvgd-channel",
      "directory": "."
    }
  ]
}
